{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "/usr/include/x86_64-linux-gnu",
                "/usr/include",
                "${workspaceRoot}/FreeRTOS/include",
                "${workspaceRoot}/FreeRTOS/portable/ThirdParty/GCC/Posix",
                "${workspaceRoot}/FreeRTOS/portable/ThirdParty/GCC/Posix/utils",
                "${workspaceRoot}/FreeRTOS/portable/MemMang",
                "${workspaceRoot}/FreeRTOS-Plus/FreeRTOS-Plus-Trace/Include",
                "${workspaceRoot}/Core/Inc",
                "${workspaceRoot}/Core/Src"
            ],
            "defines": [
                "DEBUG",
                "projCOVERAGE_TEST=0"
            ],
            "intelliSenseMode": "gcc-arm",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "browse": {
                "path": [
                    "${workspaceRoot}",
                    "${env:VSARM}/arm-gcc",
                    "${env:VSARM}/cache"
                ],
                "limitSymbolsToIncludedHeaders": false,
                "databaseFilename": ""
            }
        }
    ],
    "version": 4
}
